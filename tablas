create table if not exists artist(
	followers integer,
	genre varchar(20),
	artist_id varchar(50),
	name varchar(200),
	popularity integer
);

create table if not exists album(
	artist_id varchar(50),
	album_id varchar(50),
	name varchar(200),
	release_date date,
	total_tracks integer
);

create table if not exists track(
	album_id varchar(50),
	artist_id varchar(50),
	disc_number integer,
	duration_ms integer,
	explicit boolean
	track_id varchar(50),
	name varchar(200),
	popularity integer,
	track_number integer
);
